{
  "expo": {
    "name": "KaasproFoods",
    "slug": "delivery-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "scheme": "deliveryapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": false,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#10B981"
      },
      "softwareKeyboardLayoutMode": "pan",
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.sanjeev_s.deliveryapp"
    },
    "web": {
      "output": "static"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#10B981"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow KaasproFoods to scan table QR codes for restaurant ordering"
        }
      ],
      [
        "expo-media-library",
        {
          "photosPermission": "Allow KaasproFoods to save QR codes to your gallery",
          "savePhotosPermission": "Allow KaasproFoods to save QR codes to your gallery",
          "isAccessMediaLocationEnabled": false
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {},
      "eas": {
        "projectId": "148ec22d-bea9-48eb-a375-66ca523854b5"
      }
    }
  }
}
